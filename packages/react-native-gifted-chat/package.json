{
  "name": "react-native-gifted-chat",
  "version": "2.5.0",
  "description": "The most complete chat UI for React Native",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaridSafi/react-native-gifted-chat.git"
  },
  "keywords": [
    "android",
    "ios",
    "react-native",
    "react",
    "react-component",
    "messenger",
    "message",
    "chat"
  ],
  "author": "Farid Safi",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "./node_modules/.bin/eslint ./src/*.{ts,tsx} --fix",
    "tsc": "node_modules/.bin/tsc --noEmit",
    "tsc:watch": "node_modules/.bin/tsc --watch --noEmit",
    "start": "cd example && expo start",
    "start:web": "cd example && expo start -w --dev",
    "build": "rm -rf lib/ && node_modules/.bin/tsc && cp flow-typedefs/*.js.flow lib/",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-transform-flow-strip-types": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/react": "~18.2.0",
    "@types/react-test-renderer": "18.0.7",
    "@types/uuid": "3.4.9",
    "@typescript-eslint/parser": "8.3.0",
    "eslint-config-prettier": "8.5.0",
    "flow-bin": "0.125.0",
    "husky": "4.2.5",
    "json": "11.0.0",
    "metro-react-native-babel-preset": "0.70.1",
    "prettier": "3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-linear-gradient": "^2.8.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "homebase-id-app-common": "*",
    "@expo/react-native-action-sheet": "4.0.1",
    "@mattermost/react-native-paste-input": "0.8.0",
    "dayjs": "1.8.26",
    "prop-types": "15.7.2",
    "react-native-communications": "2.2.1",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-lightbox-v2": "0.9.0",
    "react-native-parsed-text": "0.0.22",
    "react-native-reanimated": "^3.12.1",
    "react-native-typing-animation": "0.1.7",
    "react-native-haptic-feedback": "^2.2.0",
    "uuid": "3.4.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
