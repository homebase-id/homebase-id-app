{
  "name": "homebase-feed-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android": "react-native build-android --mode=release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android-codepush": "rm -rf codepush && mkdir codepush && react-native bundle --platform android --dev false --entry-file packages/mobile/index.js --bundle-output codepush/index.android.bundle --assets-dest codepush",
    "build:ios-codepush": "rm -rf codepush && mkdir codepush && react-native bundle --platform ios --dev false --entry-file packages/mobile/index.js --bundle-output codepush/index.ios.bundle --assets-dest codepush",
    "release:android-codepush": "appcenter codepush release -a odinops/Homebase-Feed -d Staging -c ./codepush -t 0.0.1 && rm -rf codepush",
    "release:ios-codepush": "appcenter codepush release -a odinops/Homebase-Feed-ios -d Staging -c ./codepush -t 0.0.1 && rm -rf codepush"
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.5",
    "@peculiar/asn1-schema": "^2.3.6",
    "@peculiar/json-schema": "^1.1.12",
    "@peculiar/webcrypto": "^1.4.3",
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/native-stack": "^6.9.14",
    "@react-navigation/native": "^6.1.8",
    "@tanstack/query-async-storage-persister": "^5.4.3",
    "@tanstack/react-query-persist-client": "^5.4.3",
    "@tanstack/react-query": "^5.4.3",
    "axios": "^1.4.0",
    "base-64": "^1.0.0",
    "create-ecdh": "^4.0.4",
    "js-crypto-hkdf": "^1.0.5",
    "lodash-es": "^4.17.21",
    "pvtsutils": "^1.3.2",
    "react-native-code-push": "^8.1.0",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-file-access": "^3.0.4",
    "react-native-quick-crypto": "^0.6.1",
    "react-native-quick-sqlite": "^8.0.2",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^13.13.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "^13.2.3",
    "react-native": "^0.72.4",
    "react": "18.2.0",
    "stream": "^0.0.2",
    "text-encoding-polyfill": "^0.6.7",
    "tslib": "^2.5.0",
    "webcrypto-core": "^1.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/eslint-plugin": "^0.73.0",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/lodash-es": "^4.17.7",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-native-video": "^5.0.15",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2"
  },
  "engines": {
    "node": ">=16"
  }
}
